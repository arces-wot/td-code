{
	"name": "td-code",
	"displayName": "td-code",
	"description": "Editing Thing Descriptors inside vscode",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.46.0"
	},
	"icon": "images/icon.png",
	"categories": [
		"Linters",
		"Programming Languages",
		"Other"
	],
	"contributes": {
		"jsonValidation": [
			{
				"fileMatch": [
					"*td.json",
					"*td.jsonld"
				],
				"url": "./src/TD.schema.json"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arces-wot/td-code.git"
	},
	"bugs": {
		"url": "https://github.com/arces-wot/td-code/issues",
		"email": "cristiano.aguzzi@unibo.it"
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"publisher": "arces-wot",
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {}
}
